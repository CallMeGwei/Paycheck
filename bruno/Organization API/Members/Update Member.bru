meta {
  name: Update Member
  type: http
  seq: 4
}

put {
  url: {{base_url}}/orgs/{{org_id}}/members/{{user_id}}
  body: json
  auth: bearer
}

auth:bearer {
  token: {{org_member_api_key}}
}

body:json {
  {
    "role": "admin"
  }
}

docs {
  Update a member (requires owner role).
  Cannot change your own role.

  Request body:
  - role: Optional. One of "owner", "admin", "member"

  Note: To update user name/email, update the user record directly.
}
